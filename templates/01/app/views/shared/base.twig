<head>
    <title>{% block html_title %}{% endblock %}</title>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, minimum-scale=1.0, maximum-scale=1.0" />
    <meta name="description" content="{{ model.meta_desc }}">

    {% for thumbnail in view.getThumbnails(400, 400, '/assets/img/og_placeholder.png') %}
        <meta property="og:image" content="{{ thumbnail }}" />
    {% endfor %}
    <meta property="og:title" content="{% block og_title %}{{ block('html_title') }}{% endblock %}" />
    <meta property="og:description" content="{% block og_desc %}{{ block('meta_desc') }}{% endblock %}" />
    <meta property="og:url" content="{% block og_url %}{{ model.url }}{% endblock %}" />
    {% if canonical_link %}<link href="{{ canonical_link }}" rel="canonical"/>{% endif %}

    <link href="/assets/css/vendor.min.css?efa7f7c5cee919b7" rel="stylesheet" type="text/css" />

    <link href="/assets/css/vendor.min.css?c3663d3bd4d31686" rel="stylesheet" type="text/css" />
    <!--[if lte IE 9 ]>
    <link href="/assets/css/screen_p1.css?612dbedea3db258b" rel="stylesheet" type="text/css" />
    <link href="/assets/css/screen_p2.css?17f25e0c8b07053c" rel="stylesheet" type="text/css" />
    <![endif]-->
    <!--[if (gt IE 9)|!(IE)]><!--><link href="/assets/css/screen.min.css?efa7f7c5cee919b7" rel="stylesheet" type="text/css" /><!--<![endif]-->

    <link rel="shortcut icon" type="image/x-icon" href="/{{ settings.favicon.src }}"

    <!--[if lte IE 8]>
    <script src="/assets/js/html5shiv.js"></script>
    <script src="/assets/js/respond.min.js"></script>
    <![endif]-->

    <script type="text/javascript" src="/assets/js/dist/vendor.min.js?11044293cd99329c"></script>
    <script type="text/javascript" src="/assets/js/dist/core.min.js?b15d0078c493845f"></script>

